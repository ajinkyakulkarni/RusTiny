fn main() {
entry-block:
    %a = alloca
    store 2 %a
    jmp while_cond1
while_cond1:
    %2 = load %a
    %1 = cmp gt %2 0
    br %1 while_body1 while_exit1
while_body1:
    %6 = load %a
    %5 = sub %6 1
    store %5 %a
    %9 = load %a
    %8 = cmp eq %9 1
    br %8 conseq1 next1
conseq1:
    jmp while_exit1
next1:
    jmp while_cond1
while_exit1:
    jmp return
return:
    ret void
}