fn main() {
entry-block:
    %a = alloca
    store 2 %a
    jmp while_cond1
while_cond1:
    %3 = load %a
    %2 = cmp gt %3 0
    br %2 while_body1 while_exit1
while_body1:
    %7 = load %a
    %6 = sub %7 1
    store %6 %a
    %10 = load %a
    %9 = cmp eq %10 1
    br %9 conseq1 next1
conseq1:
    jmp while_exit1
next1:
    jmp while_cond1
while_exit1:
    ret void
}
