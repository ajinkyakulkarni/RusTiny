fn main() {
entry-block1:
    %a = alloca
    store 2 %a
    jmp while_cond1
while_cond1:
    %1 = load %a
    %0 = cmp gt %1 0
    br %0 while_body1 while_exit1
while_body1:
    %3 = load %a
    %2 = sub %3 1
    store %2 %a
    %5 = load %a
    %4 = cmp eq %5 1
    br %4 conseq1 next1
conseq1:
    jmp while_exit1
next1:
    jmp while_cond1
while_exit1:
    ret void
}