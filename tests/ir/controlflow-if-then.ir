fn foo() {
entry-block:
    %a = alloca
    %ret_slot = alloca
    store 2 %a
    %1 = load %a
    %0 = cmp eq %1 2
    br %0 conseq1 next1
conseq1:
    %3 = load %a
    %2 = add %3 3
    store %2 %a
    jmp next1
next1:
    %4 = load %a
    store %4 %ret_slot
    jmp return
return:
    %5 = load %ret_slot
    ret %5
}

fn main() {
entry-block:
    %a = alloca
    %0 = call foo
    store %0 %a
    ret void
}
