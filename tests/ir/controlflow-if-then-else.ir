fn bar() {
entry-block:
    %ret_slot = alloca
    store 2 %ret_slot
    jmp return
return:
    %0 = load %ret_slot
    ret %0
}

fn foo() {
entry-block:
    %a = alloca
    %ret_slot = alloca
    %2 = call bar
    %1 = cmp eq %2 2
    br %1 conseq1 altern1
conseq1:
    store 5 %0
    jmp next1
altern1:
    store 7 %0
    jmp next1
next1:
    store %0 %a
    %3 = load %a
    store %3 %ret_slot
    jmp return
return:
    %4 = load %ret_slot
    ret %4
}

fn main() {
entry-block:
    %a = alloca
    %0 = call foo
    store %0 %a
    ret void
}